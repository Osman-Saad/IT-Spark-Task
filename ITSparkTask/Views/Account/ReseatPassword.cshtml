@model ReseatPasswordViewModel
@{
	Layout = "_AuthLayout";
	ViewData["Title"] = "Login";
}
<div class="main-agileinfo">
	<div class="agileits-top">
		<form asp-action="ReseatPassword" method="post">
			<div asp-validation-summary="All"></div>
			<input class="text" type="password" asp-for="Password" placeholder="Password" required="">
			<span asp-validation-for="Password"></span>
			<input class="text" type="password" asp-for="ConfirmPassword" placeholder="Password Confirmation" required="">
			<span asp-validation-for="ConfirmPassword"></span>
			<input type="submit" value="Save">
		</form>
	</div>
</div>
@section Scripts {
	<partial name="_ValidationScriptsPartial" />
}
