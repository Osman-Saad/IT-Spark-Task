@model ForgetPasswordViewModel
@{
	Layout = "_AuthLayout";
	ViewData["Title"] = "Forget Password";
}
<div class="main-agileinfo">
	<div class="agileits-top">
		<form asp-action="SendEmail" method="post">
			<div asp-validation-summary="All"></div>
			<input class="text email" type="email" asp-for="Email" placeholder="Email" required="">
			<span asp-validation-for="Email"></span>
			<input type="submit" value="Reseat Password">
		</form>
	</div>
</div>
@section Scripts {
	<partial name="_ValidationScriptsPartial" />
}

